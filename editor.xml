<application>
  <component name="CodeInsightSettings">
    <option name="HIGHLIGHT_SCOPE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_FUNCTION_BODY" value="true" />
    <option name="COLLAPSE_METHOD_BODY" value="true" />
    <option name="COLLAPSE_CLASSES" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>